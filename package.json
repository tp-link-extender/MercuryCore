{
	"name": "mercury",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"format": "prettier -w --plugin prettier-plugin-svelte ./src",
		"prod": "pnpm lines && pnpm i && vite build",
		"package": "svelte-kit package",
		"preview": "vite preview",
		"buildview": "vite build && vite preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lines": "scc -c --no-size --no-cocomo --sort lines --exclude-dir staging --exclude-dir setup --exclude-dir PrepForUpload  --exclude-file defs.d.lua,deno.lock,pnpm-lock.yaml,mercury.yml,uno.config.ts,AppSettings.xml,ReflectionMetadata.xml,materials.json,shaders.json --exclude-ext lua,h,txt --ci -f cloc-yaml -o ./data/lines"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^3.0.0",
		"@sveltejs/kit": "^2.3.2",
		"@sveltejs/vite-plugin-svelte": "^3.0.1",
		"@types/crypto-js": "^4.2.1",
		"@types/d3-interpolate": "^3.0.4",
		"@types/node": "^20.11.0",
		"@types/nprogress": "^0.2.3",
		"@types/serviceworker": "^0.0.82",
		"@types/three": "^0.160.0",
		"@types/uuid": "^9.0.7",
		"@unocss/core": "^0.58.3",
		"@unocss/extractor-svelte": "^0.58.3",
		"@unocss/preset-tagify": "^0.58.3",
		"@unocss/transformer-directives": "^0.58.3",
		"@unocss/transformer-variant-group": "^0.58.3",
		"prettier": "^3.2.1",
		"prettier-plugin-svelte": "^3.1.2",
		"stylus": "^0.62.0",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.3",
		"svelte-preprocess": "^5.1.3",
		"sveltekit-autoimport": "^1.7.1",
		"sveltekit-superforms": "^1.13.2",
		"typescript": "^5.3.3",
		"unocss": "^0.58.3",
		"zod": "^3.22.4"
	},
	"type": "module",
	"dependencies": {
		"@threlte/core": "7.0.12",
		"@threlte/extras": "8.1.2",
		"check-disk-space": "^3.4.0",
		"crypto-js": "^4.2.0",
		"custom-protocol-check": "^1.4.0",
		"d3-interpolate": "^3.0.1",
		"dotenv": "^16.3.1",
		"esbuild": "^0.19.11",
		"lucia": "^2.7.6",
		"nanoid": "^5.0.4",
		"nprogress": "^0.2.0",
		"picocolors": "^1.0.0",
		"profane-words": "^1.5.11",
		"sharp": "^0.33.2",
		"stripe-gradient": "^1.0.1",
		"surrealdb.js": "^0.11.0",
		"svelte-inview": "^4.0.2",
		"svelte-parallax": "^0.6.0",
		"three": "^0.160.0",
		"vite": "^5.0.11",
		"vite-plugin-warmup": "^0.1.0"
	}
}
